{
	"manifest_version": 2,
	"name": "endarken",
	"minimum_chrome_version": "24.0.1307.0",
	"version": "2015.12.09",
	"icons": {
		"256": "icon-256.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["endarken.css"],
			"js": ["inject.js"],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}
