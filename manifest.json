{
	"manifest_version": 2,
	"name": "colorize",
	"minimum_chrome_version": "24.0.1307.0",
	"version": "2015.11.30",
	"icons": {
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["colorize.css"],
			"js": ["inject.js"],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"tabs",
		"<all_urls>"
	]
}
